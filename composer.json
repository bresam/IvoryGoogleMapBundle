{
    "name": "ivory/google-map-bundle",
    "type": "symfony-bundle",
    "description": "Provides a google map integration for Symfony",
    "keywords": [ "google", "map" ],
    "license": "MIT",
    "authors": [
        {
            "name": "Samuel Breu",
            "email": "samuel.breu@bresam.ch",
            "role": "Developer/Maintainer"
        },
        {
            "name": "Eric GELOEN",
            "email": "geloen.eric@gmail.com",
            "role": "Creator/Inactive"
        }
    ],
    "require": {
        "php": "^8.0",
        "symfony/framework-bundle": "^5.4 || ^6.4",
        "ivory/google-map": "^5.0"
    },
    "require-dev": {
        "roave/security-advisories": "dev-latest",
        "friendsofphp/php-cs-fixer": "^2.19 || 3.14",
        "php-http/cache-plugin": "^1.7.5",
        "php-http/guzzle6-adapter": "^1.0 || ^2.0",
        "phpunit/phpunit": "^8.5 || ^9.6",
        "phpunit/phpunit-selenium": "^8.0 || ^9.0",
        "symfony/cache": "^5.0 || ^6.0",
        "symfony/form": "^5.0 || ^6.0",
        "symfony/phpunit-bridge": "^5.0 || ^6.0",
        "symfony/templating": "^5.0 || ^6.0",
        "symfony/yaml": "^5.0 || ^6.0",
        "twig/twig": "^2.15 || ^3.5"
    },
    "autoload": {
        "psr-4": { "Ivory\\GoogleMapBundle\\": "" }
    },
    "autoload-dev": {
        "psr-4": { "Ivory\\Tests\\GoogleMap\\": "vendor/ivory/google-map/tests" }
    },
    "replace": {
        "egeloen/google-map-bundle": "^3.0.1"
    }
}
